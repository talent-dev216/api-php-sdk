{
  "name": "crmcarecloud/sdk-php",
  "config": {
    "sort-packages": true
  },
  "require": {
    "crmcarecloud/api-php-client": "^2.1",
    "kevinrob/guzzle-cache-middleware": "^3.4",
    "psr/simple-cache": "^1.0",
    "symfony/cache": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "CrmCareCloud\\Webservice\\RestApi\\Client\\SDK\\": "src/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10"
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan --memory-limit=-1"
  }
}
