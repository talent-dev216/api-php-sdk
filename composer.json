{
  "name": "crmcarecloud/sdk-php",
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^8.1",
    "crmcarecloud/api-php-client": "^3.1",
    "kevinrob/guzzle-cache-middleware": "^5.1",
    "psr/simple-cache": "^3.0",
    "symfony/cache": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "CrmCareCloud\\Webservice\\RestApi\\Client\\SDK\\": "src/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "rector/rector": "^1.0"
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan --memory-limit=-1"
  }
}
